{
    "name": "illuminated/wikipedia-grabber",
    "description": "Wikipedia/MediaWiki Grabber for Laravel.",
    "keywords": ["laravel", "wikipedia", "mediawiki", "wiki", "grabber", "parser", "wikipedia-grabber", "wikipedia-parser"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dmitry-ivanov/laravel-wikipedia-grabber/issues",
        "source": "https://github.com/dmitry-ivanov/laravel-wikipedia-grabber"
    },
    "authors": [{
        "name": "Dmitry Ivanov",
        "email": "dmitry.g.ivanov@gmail.com"
    }],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.2",
        "guzzlehttp/psr7": "^2.0",
        "illuminate/support": "^10.0",
        "ramsey/collection": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "mockery/mockery": "^1.4.4",
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Illuminated\\Wikipedia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Illuminated\\Wikipedia\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Illuminated\\Wikipedia\\WikipediaGrabberServiceProvider"
            ],
            "aliases": {
                "Wikipedia": "Illuminated\\Wikipedia\\Wikipedia",
                "MediaWiki": "Illuminated\\Wikipedia\\MediaWiki"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
